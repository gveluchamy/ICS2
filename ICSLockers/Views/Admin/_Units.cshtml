@using ICSLockers.Models

<div class="col-lg-12 p-0">
    <div class="grid">
        <div class="locker available">
            <div class="d-flex align-items-center locker-detail">
                <div class="count">@Model.LockerId</div>
                <div class="user-name">available</div>
            </div>
            <div class="locker-condition flex-column d-flex align-items-center">
                <img src="~/images/png/available.png" alt="" class="success">
                <img src="~/images/png/not-available.png" alt="" class="danger">
                <span>Available</span>
            </div>
        </div>
    </div>
</div>

