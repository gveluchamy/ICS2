@using ICSLockers.Models

<div class="unit">
    <div class="unit-count d-flex align-item-center mb-3">  
        <h3>@Model.LockerUnitNo</h3>
        <h6 class="d-flex align-items-center ml-3">Division</h6>
    </div>
    <div class="location-detail text-left">
        <h6>Row: <span>0-99</span></h6>
        <h6>Available: <span>22</span></h6>
        <h6>Location: <span>Location-2</span></h6>
    </div>
    <div class="d-flex justify-content-end location-btn">
        <button class="btn available" onclick="location.href='@Url.Action("LockerDetails", "Admin", new { lockerUnitId = Model.LockerId })'">
            Available
            @*<div class="lockers">@Model.UsedLocker/@Model.TotalLocker</div>*@
        </button>
    </div>
    
</div>
 